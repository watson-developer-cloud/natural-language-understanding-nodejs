{
  "name": "@ibm-watson/natural-language-understanding-nodejs",
  "version": "0.0.2",
  "description": "A sample Node.js app for IBM Cloud that use the IBM Watson Natural Language Understanding service",
  "repository": {
    "type": "git",
    "url": "https://github.com/watson-developer-cloud/natural-language-understanding-nodejs.git"
  },
  "engines": {
    "node": ">=8.0"
  },
  "author": "IBM Corp.",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ],
      "@babel/preset-react"
    ]
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/watson-developer-cloud/natural-language-understanding-nodejs/issues"
  },
  "scripts": {
    "start": "node server.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "npm run lint && npm run test-unit && npm run test-integration-runner",
    "test-unit": "mocha test/unit --exit",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "aphrodite": "^1.2.5",
    "babel-eslint": "^10.0.1",
    "babelify": "^10.0.0",
    "body-parser": "^1.18.3",
    "css-math": "^0.4.0",
    "deep-assign": "^2.0.0",
    "dotenv": "^6.2.0",
    "es6-promise": "^4.2.6",
    "express": "^4.16.4",
    "express-browserify": "^1.0.2",
    "express-rate-limit": "^3.3.2",
    "express-react-views": "^0.11.0",
    "express-secure-only": "^0.2.1",
    "helmet": "^3.15.1",
    "html-to-react": "^1.3.4",
    "isomorphic-fetch": "^2.2.1",
    "language-list": "0.0.3",
    "map-range": "^0.1.2",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-waypoint": "^8.1.0",
    "scroll-to-element": "^2.0.2",
    "tween": "^0.9.0",
    "uglifyify": "^5.0.1",
    "valid-url": "^1.0.9",
    "watson-developer-cloud": "^3.18.1",
    "watson-react-components": "^0.6.19",
    "watson-ui-components": "^0.6.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "casperjs": "^1.1.4",
    "codecov": "^3.2.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "phantomjs-prebuilt": "^2.1.16",
    "supertest": "^3.4.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
